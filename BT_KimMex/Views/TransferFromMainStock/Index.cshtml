<div class="well" style="padding:8px !important; padding-bottom:30px !important;">
    <div class="form-group">
        <label class="control-label col-md-2">Requested Date:</label>
        <div class="col-md-3">
            <div id="reportrange" style="background: #fff; cursor: pointer; padding: 5px 10px; border: 1px solid #ccc; width: 100%">
                <i class="fa fa-calendar"></i>&nbsp;
                <span></span> <i class="fa fa-caret-down"></i>
            </div>
        </div>
        <label class="control-label col-md-2">Status:</label>
        <div class="col-md-3">
            <select class="form-control" id="ddr_status" name="ddr_status" style="width:100% !important;">
                <option value="0">All</option>
                <option value="@Status.Pending">@ShowStatus.Created</option>
                <option value="@Status.PendingFeedback">@ShowStatus.PendingFeedback</option>
                <option value="@Status.Completed">@ShowStatus.Approved</option>
            </select>
        </div>
        <div class="col-md-2">
            <button type="button" class="w3-button w3-tiny w3-blue" onclick="initialDataTable()">Filter</button>
        </div>
    </div>
</div>

<table class="table table-bordered" id="transferFromMainStockTable">
    <thead>
        <tr>
            <th rowspan="2">ID</th>
            <th rowspan="2">Date</th>
            <th rowspan="2">Transfer No.</th>
            <th rowspan="2">PR Ref.</th>
            <th rowspan="2">Project</th>
            <th rowspan="2">To Warehouse</th>
            <th rowspan="2">Requester</th>
            <th colspan="2">Status</th>
            <th rowspan="2">Action</th>
            @*<th>Approve/ Feedback</th>
            <th>View</th>
            <th>Edit</th>
            <th>Delete</th>*@
        </tr>
        <tr>
            <th>Action</th>
            <th>GRN</th>
        </tr>
    </thead>
    <tbody>
        @*@{ 
            int count = 1;
            foreach(var item in Model)

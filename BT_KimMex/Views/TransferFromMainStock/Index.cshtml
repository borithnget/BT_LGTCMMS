@model IEnumerable<BT_KimMex.Models.TransferFromMainStockViewModel>
@using BT_KimMex.Class

@{
    ViewBag.Title = "Index";
    string status = string.Empty;
    if(string.IsNullOrWhiteSpace(Request.QueryString["status"]))
    {
        status = string.Empty;
    }
    else
    {
        status = Request.QueryString["status"].ToString();
    }
}
<link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css" />

<style type="text/css">
    #transferFromMainStockTable tr td {
        vertical-align: middle  !important;
    }

    #transferFromMainStockTable tr td:nth-child(9), transferFromMainStockTable tr td:nth-child(10), 
    #transferFromMainStockTable tr td:nth-child(11), transferFromMainStockTable tr td:nth-child(12)
    {
        text-align: center !important;
    }
</style>



<h3 class="title">Transfer From WorkShop List</h3>
